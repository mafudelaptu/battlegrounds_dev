<?php if( count($data) > 0 && is_array($data)): ?> <h3>Your Status</h3> <div class="row"> <?php $i=1; ?> <?php foreach($data as $k=>$v): ?> <?php if( $v['matchesData'] != ""): ?> <div class="col-sm-6"> <h4 class="muted">< Round <?php echo $i; ?> ></h4> <div> <strong>MatchID: </strong> <?php echo $v['matchesData']->match_id; ?> </div> <div> <strong>You are in Team: </strong> <?php echo $v['playerTeam']; ?> </div> <div> <strong>You have to Play vs. Team: </strong> <?php echo $v['opponentTeam']; ?> </div> <hr> <div> <?php echo $v['status']; ?> </div> </div> <?php $i++; ?> <?php endif; ?> <?php endforeach; ?> </div> <?php endif; ?>